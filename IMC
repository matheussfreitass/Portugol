algoritmo "CalculoIMC"
var
         M, A , IMC: Real
inicio
      Escreva ("Massa (kg): ")
      Leia (M)
      Escreva ("Altura (m): ")
      Leia (A)
      IMC <- M / (A ^ 2)
      EscrevaL ("IMC: ",IMC:5:2)
      Se (IMC >= 18.5) e (IMC <=25) Entao
           EscrevaL("Parabés!! Você está no peso ideal.")
           Senao
           EscrevaL ("Cuidado!! Você está fora do peso ideal.")
      FimSe

fimalgoritmo
